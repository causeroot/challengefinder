#!/bin/bash

python src/termExtraction/word_features.py $1 goodList.sitewords
python src/termExtraction/word_features.py $1 badList.sitewords

./runOctaveSet

./webSearch $1

./jsonStrip data/$1/urls/newlySearchedOutput.res data/$1/urls/newlySearchedOutput.url

python src/termExtraction/word_features.py $1 newUrlList.sitewords

./generateClassification $1